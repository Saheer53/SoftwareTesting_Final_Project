und {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n}\n\nnav a {\n  line-height: 3rem;\n  padding: 0 1rem;\n  text-transform: uppercase;\n  text-decoration: none;\n  color: white;\n}\n\n#top-menu a:hover {\n  background-color: var(--sub-menu-bg);\n}\n\nheader, #top-menu {\n  position: relative;\n}\n\n#top-menu {\n  z-index: 20;\n}\n\n#sub-menu {\n  width: 100%;\n  z-index: 10;\n  transition: top 0.5s ease-out;\n}\n\n#sub-menu a:hover {\n  background-color: var(--top-menu-bg);\n}\n\nnav a.active {\n  background-color: var(--sub-menu-bg);\n  color: var(--main-bg);\n}\n\n\n.row {\n  display: -ms-flexbox;\n  display: flex;\n  flex-wrap: wrap;\n  height: auto;\n  min-width: auto;\n  text-align: center;\n  /* border: black; */\n}\n\ndiv {\n  padding: 10px;\n}\n/* Style for the login modal */\n.modal {\n  display: none; /* Hidden by default */\n  position: fixed; /* Stay in place */\n  z-index: 100; /* Sit on top */\n  left: 0;\n  top: 0;\n  width: 100%; /* Full width */\n  height: 100%; /* Full height */\n  overflow: auto; /* Enable scroll if needed */\n  background-color: rgb(0,0,0); /* Fallback color */\n  background-color: rgba(0,0,0,0.4); /* Black w/ opacity */\n}\n\n.modal-content {\n  background-color: #fefefe;\n  margin: 15% auto; /* 15% from the top and centered */\n  padding: 20px;\n  border: 1px solid #888;\n  width: 80%; /* Could be more or less, depending on screen size */\n}\n\n.close {\n  color: #aaa;\n  float: right;\n  font-size: 28px;\n  font-weight: bold;\n}\n\n.close:hover,\n.close:focus {\n  color: black;\n  text-decoration: none;\n  cursor: pointer;\n}\n\n\n  /* === left =================== */\n.left {\n  width: 50%;\n}\n\n.time-slots {\n  display: none;\n}\n\n.time-slot {\n  float: left;\n  margin-bottom: 5px;\n  vertical-align: top;\n  border: 1px solid #03045E;\n  margin: 5px;\n  /* padding: 5px; */\n  width: calc(20% - 12.5px);\n  box-sizing: border-box;\n}\n\n#time-slots {\n  /* display: flex; */\n  flex-wrap: wrap;\n  overflow: hidden;\n}\n\n.appointment-details {\n  border: 1px solid #ccc;\n  padding: 20px;\n  margin-top: 20px;\n  background-color: #f9f9f9;\n}\n\n  /* === right======================= */\n.right {\n  width: 50%;\n}\n\ninput[type=text], input[type=date], input[type=tel], input[type=email], select, textarea, [type=submit] {\n  width: 100%;\n  padding: 12px;\n  margin: 8px 0;\n  display: inline-block;\n  border: 1px solid #ccc;\n  box-sizing: border-box;\n}\n\ninput::placeholder,\ntextarea::placeholder {\n  color: #ADB5BD;\n}\n\ninput[type=checkbox] {\n  margin-top: 16px;\n}\n\n.button{\n  text-decoration: none;\n  padding: 1em 2.5em; \n  background-color: #023E8A; \n  color: #fff; \n  border-radius: 0.25em;\n  font-size: 16px;\n  \n  background-image: linear-gradient(75deg, #0466C8, #023E7D, #023E8A, #0353A4); \n  background-size: 300% ;\n  background-position: left;\n  \n  transition: 300ms background-position ease-in-out;\n  cursor: pointer;\n}\n\n.button:hover{\n  background-position: right; \n  opacity: 0.8;\n}\n"},"lineCount":null}},"error":null,"hash":"8d9000171f74fe28bc3891040973cfd7","cacheData":{"env":{}}}